{
	"name": "commercial-canaries",
	"version": "0.0.1",
	"description": "repo to keep guardian's aws canaries",
	"main": "index.js",
	"repository": "git@github.com:guardian/commercial-canaries.git",
	"license": "Apache-2.0",
	"scripts": {
		"build": "tsc",
		"test": "jest --verbose",
		"test-update": "jest -u",
		"format": "prettier --write .",
		"lint": "eslint src/** && eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
		"synth": "cdk synth --path-metadata false --version-reporting false",
		"diff": "cdk diff --path-metadata false --version-reporting false"
	},
	"devDependencies": {
		"@guardian/cdk": "61.4.0",
		"@guardian/eslint-config": "11.0.0",
		"@guardian/prettier": "8.0.1",
		"@types/jest": "29.5.14",
		"@types/node": "22.13.11",
		"aws-cdk": "2.1007.0",
		"aws-cdk-lib": "2.189.1",
		"eslint": "9.31.0",
		"jest": "29.7.0",
		"prettier": "3.5.3",
		"ts-jest": "29.2.6",
		"ts-node": "10.9.2",
		"typescript": "5.7.2"
	},
	"packageManager": "pnpm@9.15.5"
}
